    "function raiz(a)\n",
    "    x=sqrt_root(a/2);\n",
    "    for i in 1:20\n",
    "        x= x-((x*tan(x)-sqrt_root(a-x^2))/(tan(x)+x*sec(x)^2+x/(sqrt_root(a-x^2))));\n",
    "    end\n",
    "    return x\n",
    "end"
   ]
  }
